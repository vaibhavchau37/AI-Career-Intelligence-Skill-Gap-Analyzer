# Example Streamlit secrets file
# Copy this file to `.streamlit/secrets.toml` and fill values locally.
# IMPORTANT: Never commit `.streamlit/secrets.toml`.

# Option A: flat keys
# OPENAI_API_KEY = "your_openai_key_here"
# GOOGLE_GEMINI_API_KEY = "your_gemini_key_here"
# SAMBANOVA_API_KEY = "your_sambanova_key_here"
# SAMBANOVA_BASE_URL = "https://api.sambanova.ai"
# SAMBANOVA_MODEL = "Meta-Llama-3.1-70B-Instruct"
# SAMBANOVA_EXTRA_HEADERS_JSON = "{\"X-Project-Id\": \"...\"}"

# Option B: nested keys
# [openai]
# api_key = "your_openai_key_here"

# [sambanova]
# api_key = "your_sambanova_key_here"
# base_url = "https://api.sambanova.ai"
# model = "Meta-Llama-3.1-70B-Instruct"
# extra_headers_json = "{\"X-Project-Id\": \"...\"}"
